<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.feng.competition.bean">

	<class name="Competition" table="cr_competition">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>	
		<property name="name" column="name"></property>
		<property name="industry" column="class"></property>
		<property name="city" column="city"></property>	
		<property name="pic" column="pic"></property>
		<property name="url" column="url"></property>
		<property name="content" column="info"></property>
		<property name="recommend" column="recommend"></property>
	</class>

</hibernate-mapping>

